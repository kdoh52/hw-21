  "dependencies": {
    "@material-ui/core": "^4.11.1",
    "axios": "^0.21.0",
    "concurrently": "^5.3.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "mongoose": "^5.10.17",
    "nodemon": "^2.0.6"
  }
